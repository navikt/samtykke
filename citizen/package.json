{
  "name": "citizen",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:css\" \"vite\"",
    "build": "npm run build:css && tsc && vite build",
    "build:mock": "npm run build:css && tsc && vite build --mode mock",
    "preview": "vite preview",
    "build:css": "tailwindcss -m -i ./src/index.css -o ./src/styles/app.css",
    "dev:css": "tailwindcss -w -i ./src/index.css -o ./src/styles/app.css",
    "e2e:run": "cypress run",
    "e2e:open": "cypress open"
  },
  "dependencies": {
    "@navikt/aksel-icons": "^4.1.7",
    "@navikt/ds-tailwind": "^4.1.5",
    "@types/cypress": "^1.1.3",
    "@types/express": "^4.17.14",
    "@types/node": "^18.11.18",
    "@types/uuid": "^9.0.0",
    "autoprefixer": "^10.4.13",
    "axios": "^1.1.3",
    "cypress": "^12.0.2",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "http-proxy-middleware": "^2.0.6",
    "msw": "^0.49.0",
    "openid-client": "^5.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.45.0",
    "react-router-dom": "^6.4.3",
    "uuid": "^9.0.0"
